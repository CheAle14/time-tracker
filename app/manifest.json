{
    "name": "Video Time Tracker",
    "description": "Tracks the time watched of videos, synced across devices",
    "version": "0.1",
    "permissions": [
        "storage",
        "activeTab",
        "tabs",
        "webNavigation"
    ],
    "manifest_version": 2,
    "browser_action": {"default_title": "Thing"},
    "content_scripts": [
        {
            "matches": [
                "https://youtube.com/*",
                "https://*.youtube.com/*"
            ],
            "js": ["contentScript.js"]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}